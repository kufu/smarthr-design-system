{
  "name": "smarthr-design-system",
  "description": "Documentation page for SmartHR's design system",
  "version": "0.1.0",
  "author": "SmartHR Design System Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsbyjs/gatsby-starter-default"
  },
  "bugs": {
    "url": "https://github.com/gatsbyjs/gatsby/issues"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix"
    ],
    "src/**/*.ts{,x}": [
      "stylelint"
    ],
    "content/**/*.md{,x}": [
      "textlint"
    ]
  },
  "scripts": {
    "build": "GATSBY_EXPERIMENTAL_PAGE_BUILD_ON_DATA_CHANGES=true gatsby build --log-pages",
    "dev": "gatsby develop",
    "start": "yarn dev",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "lint:ts": "eslint 'src/**/*.ts{,x}'",
    "lint:style": "stylelint 'src/**/*.ts{,x}'",
    "lint:text": "textlint 'content/**/*.md{,x}'",
    "lint": "run-p lint:*",
    "format": "eslint --fix 'src/**/*.ts{,x}'",
    "format:text": "textlint --fix 'content/**/*.md{,x}'",
    "typegen": "IS_TYPE_GEN=true gatsby build",
    "export:ui-props": "ts-node scripts/exportUIProps.ts",
    "export:zip-images": "ts-node scripts/zipImages.ts",
    "postinstall": "husky install"
  },
  "dependencies": {
    "@types/marked": "^4.0.8",
    "algoliasearch": "^4.14.3",
    "color": "^4.2.3",
    "dotenv": "^16.0.3",
    "gatsby": "^4.25.3",
    "gatsby-plugin-algolia": "^1.0.2",
    "gatsby-plugin-google-gtag": "^4.25.0",
    "gatsby-plugin-mdx": "^3.20.0",
    "gatsby-source-airtable": "^2.3.0",
    "marked": "^4.2.12",
    "react": "^18.2.0",
    "react-docgen-typescript": "^2.2.2",
    "react-dom": "^18.2.0",
    "react-helmet": "^6.1.0",
    "react-instantsearch-dom": "^6.38.3",
    "react-live": "^3.1.1",
    "react-string-replace": "^1.1.0",
    "smarthr-patterns": "kufu/smarthr-patterns#main",
    "smarthr-ui": "^25.1.1",
    "styled-components": "^5.3.6"
  },
  "devDependencies": {
    "@mdx-js/mdx": "^1.6.22",
    "@mdx-js/react": "^1.6.22",
    "@textlint/textlint-plugin-markdown": "^12.6.0",
    "@types/adm-zip": "^0.5.0",
    "@types/color": "^3.0.3",
    "@types/mdx-js__react": "^1.5.5",
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "@types/react-helmet": "^6.1.6",
    "@types/react-instantsearch-dom": "^6.12.3",
    "@types/styled-components": "^5.1.26",
    "adm-zip": "^0.5.10",
    "eslint": "^8.32.0",
    "eslint-config-smarthr": "6.6.1",
    "eslint-plugin-mdx": "^2.0.5",
    "eslint-plugin-react-hooks": "^4.6.0",
    "gatsby-plugin-manifest": "^4.25.0",
    "gatsby-plugin-react-helmet": "^5.25.0",
    "gatsby-plugin-sharp": "^4.25.0",
    "gatsby-plugin-styled-components": "^5.25.0",
    "gatsby-plugin-typegen": "^3.1.0",
    "gatsby-remark-emoji": "^0.0.3",
    "gatsby-remark-image-attributes": "^1.1.0",
    "gatsby-remark-images": "^6.25.0",
    "gatsby-remark-relative-images": "^2.0.2",
    "gatsby-source-filesystem": "^4.25.0",
    "glob": "^8.1.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.1.0",
    "micromark": "^3.1.0",
    "micromark-extension-mdxjs": "^1.0.0",
    "npm-run-all": "^4.1.5",
    "postcss-jsx": "^0.36.4",
    "postcss-syntax": "^0.36.2",
    "postinstall-postinstall": "^2.1.0",
    "prettier": "2.8.3",
    "prism-react-renderer": "^1.3.5",
    "remark-emoji": "^2.2.0",
    "smarthr-normalize-css": "^1.1.0",
    "stylelint": "^14.16.1",
    "stylelint-config-prettier": "^9.0.4",
    "stylelint-config-smarthr": "^1.1.0",
    "stylelint-config-standard": "^29.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "textlint": "^12.5.1",
    "textlint-filter-rule-allowlist": "^4.0.0",
    "textlint-filter-rule-comments": "^1.2.2",
    "textlint-rule-preset-smarthr": "^1.17.2",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4",
    "unist-util-visit": "2.0.3"
  },
  "resolutions": {
    "postcss": "^8.4.21"
  }
}
